[sub_resource type="GDScript" id=12]
script/source = "extends Popup


func _ready():
	$ColorRect/Popup1.visible = true

func _input(event):
	if event is InputEventScreenTouch && event.is_pressed():
		if visible:
			if G.Popups == 1:
				if $ColorRect/Popup1/popup2.visible == false:
					$ColorRect/Popup1/popup2.visible = true
					$ColorRect/Popup1/RichTextLabel2.visible = true
					$ColorRect/Popup1/ColorRect2.visible = true
					$ColorRect/Popup1/ColorRect.visible = false
				else:
					$ColorRect/Popup1.visible = false
					$ColorRect/Popup2.visible = true
					close()
			if G.Popups == 2:
				if $ColorRect/Popup2/popup2.visible == false:
					$ColorRect/Popup2/popup2.visible = true
					$ColorRect/Popup2/RichTextLabel2.visible = true
					$ColorRect/Popup2/ColorRect2.visible = true
					$ColorRect/Popup2/ColorRect.visible = false
				elif $ColorRect/Popup2/popup3.visible == false:
					$ColorRect/Popup2/popup3.visible = true
					$ColorRect/Popup2/RichTextLabel3.visible = true
					$ColorRect/Popup2/ColorRect3.visible = true
					$ColorRect/Popup2/ColorRect2.visible = false
				else:
					$ColorRect/Popup2.visible = false
					$ColorRect/Popup3.visible = true
					close()
			if G.Popups == 3:
				$ColorRect/Popup3.visible = false
				close()

func open():
	$\"/root/World/Interface/Control\".visible = false
	$\"/root/World/Interface/Menus/Pause\".visible = false

func close():
	hide()
	get_tree().paused = false
	$\"/root/World/Interface/Control\".visible = true
	$\"/root/World/Interface/Menus/Pause\".visible = true
"

